{
  "name": "my-cv",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate": "resume export resume/dist/%THEME%/%THEME%.pdf -r resume/fr/resume.json --theme %THEME% && resume export resume/dist/%THEME%/%THEME%.html -r resume/fr/resume.json --theme %THEME%",
    "generate-kendall": "cross-env THEME=kendall npm run generate",
    "generate-elegant": "cross-env THEME=elegant npm run generate",
    "generate-cora": "cross-env THEME=cora npm run generate",
    "generate-rickosborne": "cross-env THEME=rickosborne npm run generate",
    "generate-paper": "cross-env THEME=paper npm run generate",
    "generate-even": "resume export resume/dist/%THEME%/%THEME%.html -r resume/fr/resume.json",
    "generate-microdata": "cross-env THEME=microdata npm run generate",
    "resumed": "resumed render -r resume/fr/resume.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@ngx-translate/core": "^16.0.4",
    "jsonresume-theme-classy": "^1.0.9",
    "jsonresume-theme-cora": "^0.1.1",
    "jsonresume-theme-elegant": "^1.16.1",
    "jsonresume-theme-even": "^0.23.0",
    "jsonresume-theme-kendall": "^0.2.0",
    "jsonresume-theme-microdata": "*",
    "jsonresume-theme-paper": "*",
    "jsonresume-theme-rickosborne": "*",
    "resumed": "^4.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2"
  }
}
